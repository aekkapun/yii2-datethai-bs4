    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "datepicker",
		"calendar",
		"buddhist",
		"thai"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "aekkapun",
            "email": "aekkapuncs@gmail.com",
            "role": "Beginner Developer"
        }
    ],    
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/bootstrap-datepicker": "1.7.0"
    },
    "autoload": {
        "psr-4": {
            "aekkapun\\datepicker\\": "src"
        }
